<!-- inline scripts related to this page -->
<script type="text/javascript">
    jQuery(function($) {
        $.ajax({
            url:"/page/dashboard"
        }).done(function(data){
            $(".page-content").html(data);
        });

        $(".temp-page").click(function(e){
            var target = $(this).attr("href").replace("#","");
            var that = this;
            $.ajax({
                url:"/page/"+target
            }).done(function(data){
                if(data.error==502){
                    $('#myModal').modal('toggle');
                } else {
                    $(".page-content").html(data);
                }
            });
        });

        $("#btn_login").click(function(e){
            $("#passwd").val(faultylabs.MD5($("#passwd").val()));
            $("#login_form").submit();
            $('#myModal').modal('toggle');
        });
    });
</script>
