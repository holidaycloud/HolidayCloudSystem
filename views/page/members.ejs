<!-- /section:settings.box -->
<div class="page-header">
    <h1>
        首页
        <small>
            <i class="ace-icon fa fa-angle-double-right"></i>
            我的企业
        </small>
        <small>
            <i class="ace-icon fa fa-angle-double-right"></i>
            账号管理
        </small>
    </h1>
</div><!-- /.page-header -->

<div class="row">
    <div class="col-xs-12">
        <div>
            <table id="sample-table-2" class="table table-striped table-bordered table-hover">
                <thead>
                <tr>
                    <th>企业</th>
                    <th>手机号</th>
                    <th class="hidden-480">用户名</th>
                    <th class="hidden-480">邮箱</th>
                    <th>状态</th>
                    <th>
                        <i class="ace-icon fa fa-clock-o bigger-110 hidden-480"></i>
                        创建日期
                    </th>
                    <th></th>
                </tr>
                </thead>

                <tbody>
                <%members.forEach(function(m){%>
                <tr>
                    <td>
                        <%=m.ent.name%>
                    </td>
                    <td><%=m.mobile%></td>
                    <td class="hidden-480"><%=m.loginName%></td>
                    <th class="hidden-480"><%=m.email%></th>
                    <th><%=m.isEnable?'启用':'禁用'%></th>
                    <td>
                        <%=new Date(m.createDate).Format("yyyy-MM-dd hh:mm:ss")%>
                    </td>
                    <td>
                        <div class=action-buttons">
                            <a class="blue" href="#">
                                <i class="ace-icon fa fa-search-plus bigger-130"></i>
                            </a>
                            <a class="green" href="#">
                                <i class="ace-icon fa fa-pencil bigger-130"></i>
                            </a>
                            <a class="red" href="#">
                                <i class="ace-icon fa fa-trash-o bigger-130"></i>
                            </a>
                        </div>
                    </td>
                </tr>
                <%});%>
                </tbody>
            </table>
        </div>
    </div><!-- /.col -->
</div><!-- /.row -->
<script>
    jQuery(function($) {
        var oTable1 = $('#sample-table-2').dataTable( {
            bAutoWidth: false,
            "aoColumns": [
                null, null,null, null, null,null, { "bSortable": false }
            ]
        } );
    })
</script>